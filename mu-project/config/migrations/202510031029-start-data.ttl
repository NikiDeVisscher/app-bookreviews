review:review1 a schema:Review ;
    mu:uuid "review-1-uuid" ;
    schema:reviewContent "A classic novel that explores themes of decadence and excess in the Jazz Age. A must-read!" ;
    schema:reviewRating 5 ;
    schema:author reader:reader1 ;
    schema:about book:book1 ;
    schema:dateCreated "2020-01-15"^^xsd:date .

review:review2 a schema:Review ;
    mu:uuid "review-2-uuid" ;
    schema:reviewContent "As a reader you are never allowed to feel with Tom Robinson, the Black man who is innocently convicted for raping a white woman, because all the Black characters in this tale are sidelined. Sadly, I cannot recommend this book." ;
    schema:reviewRating 1 ;
    schema:author reader:reader2 ;
    schema:about book:book2 ;
    schema:dateCreated "2021-03-22"^^xsd:date .

review:review3 a schema:Review ;
    mu:uuid "review-3-uuid" ;
    schema:reviewContent "While the plot was very gripping and well-written, the book didn't actually instruct me on how to kill a mockingbird." ;
    schema:reviewRating 2 ;
    schema:author reader:reader1 ;
    schema:about book:book2 ;
    schema:dateCreated "2021-07-10"^^xsd:date .

review:review4 a schema:Review ;
    mu:uuid "review-4-uuid" ;
    schema:reviewContent "A chilling depiction of a dystopian future that feels eerily relevant today. Orwell's vision is both terrifying and thought-provoking." ;
    schema:reviewRating 5 ;
    schema:author reader:reader3 ;
    schema:about book:book3 ;
    schema:dateCreated "2019-11-05"^^xsd:date .

review:review5 a schema:Review ;
    mu:uuid "review-5-uuid" ;
    schema:reviewContent "1984 is a profound novel that delves into the dangers of totalitarianism and extreme political ideology. A must-read for everyone." ;
    schema:reviewRating 5 ;
    schema:author reader:reader1 ;
    schema:about book:book3 ;
    schema:dateCreated "2020-06-18"^^xsd:date .

